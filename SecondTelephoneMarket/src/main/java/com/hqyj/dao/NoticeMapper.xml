<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hqyj.dao.NoticeMapper">
	<resultMap id="notice" type="com.hqyj.entity.Notice">
		<result column="n_id" property="n_id"></result>
		<result column="notices" property="notices"></result>
		<result column="time" property="time"></result>
<result column="url" property="url"></result>
	</resultMap>

<select id="selectAllNotice" resultMap="notice">
select *from notice
</select>

</mapper>