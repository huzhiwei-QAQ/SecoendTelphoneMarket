<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hqyj.dao.SupplierMapper">
	<resultMap id="supplier" type="com.hqyj.entity.Supplier">
		<result column="s_id" property="s_id"></result>
		<result column="suppliers" property="suppliers"></result>
		<result column="address" property="address"></result>

	</resultMap>



<select id="selectAllSupplier"   resultMap="supplier">
select *from supplier
</select>
</mapper>